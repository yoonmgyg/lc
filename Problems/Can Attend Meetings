/*
Determine if there are no overlaps by sorting the array and checking the end and beginning of each interval
*/
class Solution {
  boolean canAttendMeetings(vector<int> intervals) {
    sort(intervals.begin(), intervals.end());
    for (int i = 1; i < intervals.size(); ++i) {
        if (intervals[i][0] < intervals[i-1][1] {
            return false;
        }
    }
    return true;
  }
};
