// get element making up majority of elements
class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int n = nums.size();
        unordered_map<int, int> m;
        // enter number of elements into map        
        for(int i = 0; i < n; i++){
            m[nums[i]]++;
        }
        n = n / 2;
        // loop through map elements and check if greater than size / 2
        for(auto x : m){
            if (x.second > n){
                return x.first;
            }
        }
        return 0;
    }
};
